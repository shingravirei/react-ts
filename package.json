{
	"name": "react-ts",
	"version": "0.1.0",
	"author": "shingravirei",
	"license": "MIT",
	"homepage": "https://github.com/shingravirei/react-ts#readme",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "vite build",
		"test": "vitest -w",
		"serve": "vite preview",
		"check": "tsc",
		"prepare": "husky install",
		"commit": "commit"
	},
	"dependencies": {
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-router": "6.8.0",
		"react-router-dom": "6.8.0"
	},
	"devDependencies": {
		"@commitlint/cli": "17.4.2",
		"@commitlint/config-conventional": "17.4.2",
		"@testing-library/react": "13.4.0",
		"@types/react": "18.0.27",
		"@types/react-dom": "18.0.10",
		"@vitejs/plugin-react-swc": "^3.1.0",
		"c8": "7.12.0",
		"husky": "8.0.3",
		"jsdom": "21.1.0",
		"lint-staged": "13.1.0",
		"prettier": "2.8.3",
		"rome": "^11.0.0",
		"typescript": "4.9.5",
		"unocss": "^0.49.4",
		"vite": "4.1.1",
		"vitest": "0.28.4"
	},
	"commitlint": {
		"extends": [
			"@commitlint/config-conventional"
		]
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"npx rome check src",
			"npx rome format src  --write"
		]
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
