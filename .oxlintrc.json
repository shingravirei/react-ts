{
	"$schema": "./node_modules/oxlint/configuration_schema.json",
	"env": {
		"browser": true,
		"esnext": true
	},
	"globals": {},
	"settings": {},
	"plugins": [
		"react",
		"unicorn",
		"typescript",
		"oxc",
		"eslint-plugin-vitest",
		"eslint-plugin-jsx-a11y"
	],
	"categories": {
		"correctness": "error",
		"perf": "warn",
		"suspicious": "warn",
		"pedantic": "warn",
		"style": "warn"
	},

	"rules": {
		"react-in-jsx-scope": "off",
		"sort-imports": "off"
	},
	"ignores": ["src/routeTree.gen.ts"],
	"overrides": []
}
